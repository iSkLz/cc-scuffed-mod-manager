  "bugs": {
    "url": "https://github.com/iSkLz/cc-scuffed-mod-manager/issues"
  },
  "homepage": "https://github.com/iSkLz/cc-scuffed-mod-manager#readme",
  "dependencies": {
    "jquery": "^3.5.1",
    "materialize-css": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.3",
    "http-server": "^0.12.3",
    "sass": "^1.27.0",
    "typescript": "^4.0.3"
  }
}
